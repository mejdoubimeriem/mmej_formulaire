<div class="custom-contact-form">
  <h2>{{ 'Formulaire de Contact'|t }}</h2>
  <div class="form-wrapper">
    <div class="form-group">
      {{ form.nom }}
    </div>
    <div class="form-group">
      {{ form.email }}
    </div>
    <div class="form-group">
      {{ form.message }}
    </div>
    <div class="submit-wrapper">
      {{ form.actions }}
    </div>
  </div>
</div>